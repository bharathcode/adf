{
	"name": "appmetricoutput",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Json2",
						"type": "DatasetReference"
					},
					"name": "appmetricstagingsource"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "Json3",
						"type": "DatasetReference"
					},
					"name": "appmetricoutput"
				}
			],
			"transformations": [
				{
					"name": "selectcolumns"
				}
			],
			"scriptLines": [
				"source(output(",
				"          total as integer,",
				"          minimum as integer,",
				"          maximum as integer,",
				"          average as integer,",
				"          resourceId as string,",
				"          time as string,",
				"          metricName as string,",
				"          timeGrain as string,",
				"          count as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     purgeFiles: true,",
				"     documentForm: 'documentPerLine') ~> appmetricstagingsource",
				"appmetricstagingsource select(mapColumn(",
				"          average,",
				"          time,",
				"          metricName",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> selectcolumns",
				"selectcolumns sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> appmetricoutput"
			]
		}
	}
}