{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dp203azuredatafactory2024"
		},
		"AzureSynapseAnalytics_log_csv_dedicated_Destination_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSynapseAnalytics_log_csv_dedicated_Destination'"
		},
		"log_csv_ADLS_Gen2_Source_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'log_csv_ADLS_Gen2_Source'"
		},
		"dp203sqldatabase_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'dp203sqldatabase'"
		},
		"log_csv_ADLS_Gen2_Source_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dp203storageaccount2024.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dp203azuredatafactory2024_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSynapseAnalytics_log_csv_dedicated_Destination_connectionString": {
						"value": "[parameters('AzureSynapseAnalytics_log_csv_dedicated_Destination_connectionString')]"
					},
					"log_csv_ADLS_Gen2_Source_accountKey": {
						"value": "[parameters('log_csv_ADLS_Gen2_Source_accountKey')]"
					},
					"dp203sqldatabase_connectionString": {
						"value": "[parameters('dp203sqldatabase_connectionString')]"
					},
					"log_csv_ADLS_Gen2_Source_properties_typeProperties_url": {
						"value": "[parameters('log_csv_ADLS_Gen2_Source_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}